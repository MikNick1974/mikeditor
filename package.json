{
  "name": "space-haven-editor",
  "description": "The first online savegame editor for Space Haven",
  "version": "0.1.0",
  "private": true,
  "license": "EUPL-1.2",
  "dependencies": {
    "@koale/useworker": "^4.0.2",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "browser-fs-access": "^0.17.2",
    "clsx": "^1.1.1",
    "history": "^5.0.0",
    "lucide-react": "^0.15.17",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.18.0",
    "react-router-dom": "^6.0.0-beta.0",
    "react-scripts": "4.0.3",
    "rfdc": "^1.3.0",
    "web-vitals": "^1.1.2",
    "xml-js": "^1.6.11"
  },
  "scripts": {
    "dev": "react-scripts start",
    "build": "cross-env BUILD_PATH=docs react-scripts build",
    "extract-lang": "formatjs extract src/**/[A-Z]*.js --out-file src/lang/en-US.json",
    "compile-lang": "formatjs compile-folder --ast src/lang src/lang/compiled",
    "postcompile-lang": "node scripts/createLangExport"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not IE 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@formatjs/cli": "^4.2.15",
    "braces": "^3.0.2",
    "cross-env": "^7.0.3"
  }
}
