{
  "name": "space-haven-editor",
  "description": "The first online savegame editor for Space Haven",
  "version": "0.2.1",
  "private": true,
  "license": "EUPL-1.2",
  "dependencies": {
    "@koale/useworker": "^4.0.2",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^14.1.1",
    "browser-fs-access": "^0.29.4",
    "buffer": "^6.0.3",
    "clsx": "^1.1.1",
    "history": "^5.3.0",
    "lucide-react": "^0.35.0",
    "node-stream-zip": "^1.15.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-intl": "^5.25.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "rfdc": "^1.3.0",
    "web-vitals": "^2.1.4",
    "xml-js": "^1.6.11"
  },
  "scripts": {
    "dev": "react-scripts start",
    "build": "cross-env BUILD_PATH=docs react-scripts build",
    "extract-lang": "formatjs extract src/**/[A-Z]*.js --out-file src/lang/en-US.json",
    "compile-lang": "formatjs compile-folder --ast src/lang src/lang/compiled",
    "postcompile-lang": "node scripts/createLangExport",
    "generate-credits": "npx @opengovsg/credits-generator"
  },
  "browserslist": {
    "production": [
      ">1%",
      "not dead",
      "not IE 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@formatjs/cli": "^4.2.15",
    "babel-plugin-formatjs": "^10.3.20",
    "braces": "^3.0.2",
    "cross-env": "^7.0.3",
    "eslint-plugin-postcss-modules": "^2.0.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.4.4"
  },
  "overrides": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  }
}
